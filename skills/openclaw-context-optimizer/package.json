{
  "name": "openclaw-context-optimizer",
  "version": "1.0.0",
  "description": "Intelligent context compression for OpenClaw agents - reduce token usage by 40-60%",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "setup": "node src/setup.js",
    "dashboard": "node src/dashboard.js",
    "test": "node --test"
  },
  "keywords": [
    "openclaw",
    "context-optimization",
    "token-compression",
    "x402",
    "agent-economy",
    "ai",
    "cost-reduction"
  ],
  "author": "AtlasPA",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "express": "^4.18.2",
    "commander": "^12.0.0"
  },
  "openclaw": {
    "skill": true,
    "hooks": {
      "request:before": "hooks/request-before.js",
      "request:after": "hooks/request-after.js",
      "session:end": "hooks/session-end.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AtlasPA/openclaw-context-optimizer.git"
  },
  "homepage": "https://github.com/AtlasPA/openclaw-context-optimizer#readme"
